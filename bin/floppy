#!/usr/bin/env bash

f=3 b=4
for j in f b; do
  for i in {0..7}; do
    printf -v $j$i %b "\e[${!j}${i}m"
  done
done
bld=$'\e[1m'
rst=$'\e[0m'
inv=$'\e[7m'

cat << FLOPPY
${rst}\
${f1}╭─────────────╮ ${f2}╭─┬─┬─────┬───⸜ ${f3}╭─────────────╮ ${f4}╭─┬─┬─────┬───⸜ ${f5}╭─────────────╮ ${f6}╭─┬─┬─────┬───⸜
${f1}│□│╌╌╌╌╌╌╌╌╌│□│ ${f2}│ │ │ ▒   │   │ ${f3}│□│╌╌╌╌╌╌╌╌╌│□│ ${f4}│ │ │ ▒   │   │ ${f5}│□│╌╌╌╌╌╌╌╌╌│□│ ${f6}│ │ │ █   │   │
${f1}│ │╌╌╌╌╌╌╌╌╌│ │ ${f2}│ ╰─╰─────╯   │ ${f3}│ │╌╌╌╌╌╌╌╌╌│ │ ${f4}│ ╰─╰─────╯   │ ${f5}│ │╌╌╌╌╌╌╌╌╌│ │ ${f6}│ ╰─╰─────╯   │
${f1}│ │╌╌╌╌╌╌╌╌╌│ │ ${f2}│ ┌─────────┐ │ ${f3}│ │╌╌╌╌╌╌╌╌╌│ │ ${f4}│ ┌─────────┐ │ ${f5}│ │╌╌╌╌╌╌╌╌╌│ │ ${f6}│ ┌─────────┐ │
${f1}│ └─────────┘ │ ${f2}│ │╌╌╌╌╌╌╌╌╌│ │ ${f3}│ └─────────┘ │ ${f4}│ │╌╌╌╌╌╌╌╌╌│ │ ${f5}│ └─────────┘ │ ${f6}│ │╌╌╌╌╌╌╌╌╌│ │
${f1}│   ╭─────╮─╮ │ ${f2}│ │╌╌╌╌╌╌╌╌╌│ │ ${f3}│   ╭─────╮─╮ │ ${f4}│ │╌╌╌╌╌╌╌╌╌│ │ ${f5}│   ╭─────╮─╮ │ ${f6}│ │╌╌╌╌╌╌╌╌╌│ │
${f1}│   │   ▒ │ │ │ ${f2}│□│╌╌╌╌╌╌╌╌╌│□│ ${f3}│   │   ▒ │ │ │ ${f4}│□│╌╌╌╌╌╌╌╌╌│□│ ${f5}│   │   ▒ │ │ │ ${f6}│□│╌╌╌╌╌╌╌╌╌│□│
${f1}‵───┴─────┴─┴─╯ ${f2}╰─────────────╯ ${f3}‵───┴─────┴─┴─╯ ${f4}╰─────────────╯ ${f5}‵───┴─────┴─┴─╯ ${f6}╰─────────────╯\
${rst}
FLOPPY

