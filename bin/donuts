#!/usr/bin/env bash
# donuts :(

f=3 b=4
for j in f b; do
  for i in {0..7}; do
    printf -v $j$i %b "\e[${!j}${i}m"
  done
done
bld=$'\e[1m'
rst=$'\e[0m'
inv=$'\e[7m'

cat << EOF
${rst}
${f1}    ▄▄▄▄▄▄      ${f2}    ▄▄▄▄▄▄      ${f3}    ▄▄▄▄▄▄      ${f4}    ▄▄▄▄▄▄      ${f5}    ▄▄▄▄▄▄      ${f6}    ▄▄▄▄▄▄
${f1}  ▄▀  ${bld}▄▄▄ ${f1}▀▄    ${f2}  ▄▀  ${bld}▄▄▄ ${f2}▀▄    ${f3}  ▄▀  ${bld}▄▄▄ ${f3}▀▄    ${f4}  ▄▀  ${bld}▄▄▄ ${f4}▀▄    ${f5}  ▄▀  ${bld}▄▄▄ ${f5}▀▄    ${f6}  ▄▀  ${bld}▄▄▄ ${f6}▀▄
${f1}  █   ${bld}█▄█  ${f1}█    ${f2}  █   ${bld}█▄█  ${f2}█    ${f3}  █   ${bld}█▄█  ${f3}█    ${f4}  █   ${bld}█▄█  ${f4}█    ${f5}  █   ${bld}█▄█  ${f5}█    ${f6}  █   ${bld}█▄█  ${f6}█
${f1}  ▀▄      ▄▀    ${f2}  ▀▄      ▄▀    ${f3}  ▀▄      ▄▀    ${f4}  ▀▄      ▄▀    ${f5}  ▀▄      ▄▀    ${f6}  ▀▄      ▄▀
${f1}    ▀▀▀▀▀▀      ${f2}    ▀▀▀▀▀▀      ${f3}    ▀▀▀▀▀▀      ${f4}    ▀▀▀▀▀▀      ${f5}    ▀▀▀▀▀▀      ${f6}    ▀▀▀▀▀▀
${rst}
EOF

